{
  "name": "medable-dev-tools",
  "main": "./lib/medable-dev-tools",
  "version": "0.2.0",
  "description": "Tools for working with Medable Cortex scripts and versioning orgs in source control",
  "keywords": [],
  "activationCommands": {
    "atom-workspace": [
      "medable-dev-tools:pull",
      "medable-dev-tools:push",
      "medable-dev-tools:forcePull"
    ]
  },
  "repository": "https://github.com/timrsmith/medable-dev-tools",
  "license": "MIT",
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "dependencies": {
    "async": "^2.1.2",
    "atom-space-pen-views": "^2.2.0",
    "fs-plus": "^2.9.3",
    "jsonfile": "^2.4.0",
    "junk": "^2.0.0",
    "moment": "^2.15.2",
    "pluralize": "^3.0.0",
    "request": "^2.76.0",
    "sanitize-filename": "^1.6.1",
    "space-pen": "^5.1.2",
    "underscore": "^1.8.3",
    "underscore.deep": "^0.5.1"
  },
  "configSchema": {
    "env": {
      "type": "string",
      "title": "Environment",
      "default": "api.dev.medable.com",
      "description": "The Medable environment you wish to connect to (e.g. Development or Production).",
      "enum": [
        {
          "value": "api.dev.medable.com",
          "description": "Development (recommended)"
        },
        {
          "value": "api.medable.com",
          "description": "Production (careful!)"
        }
      ],
      "order": 0
    },
    "org": {
      "type": "string",
      "title": "Org Name",
      "default": "your-org",
      "description": "The the unique name for your org. This is the org name you provided when signing up.",
      "order": 1
    },
    "apiKey": {
      "type": "string",
      "title": "API Key",
      "default": "API Key",
      "description": "The api key for connecting to your org.",
      "order": 2
    }
  }
}
